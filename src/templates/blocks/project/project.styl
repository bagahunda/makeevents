.project-profile
  padding: 0;

  .catalog__header
    padding: 1.875em 1.875em 3.75em;
    padding-bottom: 10px;
    background: white;

    -moz-box-shadow: inset 0 -10px 10px -10px #000000;
    -webkit-box-shadow: inset 0 -10px 10px -10px #000000;
    box-shadow: inset 0 -20px 15px -10px rgba(150,150,150, 0.1);

  .catalog__content
    padding: 1.875em 1.875em 3.75em;
    padding-top: 10px;

    .main-photo
      display: block
      margin: 20px 0;

.project-photos-list
  margin: 0px 0px 30px 0px;
  padding: 0
  list-style-type: none
  overflow: hidden

  li 
    display: inline-block
    width: 270px;
    height: 170px;
    margin-top: 30px;
    margin-right: 30px;
    overflow: hidden
    position: relative

  img
    width: 100%
    min-height: 100%;
    
.project-profile div, .project-profile p, .project-profile span, .project-profile a
  word-break: break-word;